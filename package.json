{
  "name": "@kupiki/backend",
  "version": "1.0.0",
  "description": "Kupiki Hotspot Backend",
  "author": "Pi Home Server",
  "homepage": "http://www.pihomeserver.fr",
  "copyright": "Copyright",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets env,stage-0\"",
    "build": "babel src -s -D -d dist --presets env,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true
      }
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "body-parser": "^1.13.3",
    "child-process-promise": "^2.2.1",
    "composable-middleware": "^0.3.0",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "diskspace": "^2.0.0",
    "express": "^4.13.3",
    "express-jwt": "^5.3.1",
    "express-sequelize-session": "0.4.0",
    "express-session": "^1.11.3",
    "fast-json-patch": "^1.0.0",
    "jsonwebtoken": "^7.0.0",
    "morgan": "^1.8.0",
    "mysql": "^2.15.0",
    "passport": "~0.3.0",
    "passport-local": "^1.0.0",
    "resource-router-middleware": "^0.6.0",
    "sequelize": "^3.32.1",
    "socket.io": "^1.3.5",
    "socket.io-client": "^1.3.5",
    "socketio-jwt": "^4.2.0",
    "sqlite3": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
